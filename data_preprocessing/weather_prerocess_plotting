import pandas as pd
from datetime import datetime
import matplotlib.pyplot as plt
df = pd.read_csv('data.csv')
print(df.columns)
print(df.isnull().sum())#checking if dataset has null values

def extract_day(date_string):
    date_obj = datetime.strptime(date_string[:13], "%Y-%m-%d %H")
    return date_obj.day

def meteo_temperature_print(df, column):
    df['date'] = df['date'].apply(lambda x: extract_day(x))
    plt.plot(df['date'], df['temperature_2m'])
    plt.xlabel('Date')
    plt.ylabel('Temperature(Â°C)')
    plt.title("Date VS Temeprature")
    plt.show()
meteo_temperature_print(df, None)    
